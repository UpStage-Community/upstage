{
    "rules": {
        "camelcase": 1,
        "consistent-this": [1, "self"],
        "new-cap": [
            1,
            {
                "capIsNewExceptions": ["Deferred"]
            }
        ],
        "no-bitwise": 2,
        "no-lonely-if": 1,
        "no-nested-ternary": 1,
        "no-new-object": 2,
        "no-plusplus": 0,
        "no-underscore-dangle": 0,
        "one-var": [1, "never"],
        "quotes": 0
    }
}
